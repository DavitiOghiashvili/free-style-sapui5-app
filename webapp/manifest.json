{
  "_version": "1.65.0",
  "sap.app": {
    "id": "freestylesapui5app",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "dataSources": {
      "mainService": {
        "uri": "http://localhost:4004/v2/root/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "fe9e3d3d-1f42-46af-a784-571bd2d7abba"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.136.1",
      "libs": {
        "sap.f": {},
        "sap.m": {},
        "sap.ui.core": {},
        "sap.uxap": {}
      }
    },
    "config": {
			"fullWidth": true
		},
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "freestylesapui5app.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "controlId": "flexibleColumnLayout",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "freestylesapui5app.view",
        "async": true
      },
      "routes": [
        {
          "pattern": ":layout:",
          "name": "ListReport",
          "target": [
            "ListReport",
            "ObjectPage"
          ]
        },
        {
          "name": "ObjectPage",
          "pattern": "ObjectPage/{Product_ID}/{layout}",
          "target": [
            "ListReport",
            "ObjectPage"
          ]
        },
        {
          "name": "ObjectChartPage",
          "pattern": "ObjectPage/{Product_ID}/ObjectChartPage/{layout}",
          "target": [
            "ListReport",
            "ObjectPage",
            "ObjectChartPage"
          ]
        }
      ],
      "targets": {
        "ListReport": {
          "name": "ListReport",
          "controlAggregation": "beginColumnPages"
        },
        "ObjectPage": {
          "name": "ObjectPage",
          "controlAggregation": "midColumnPages"
        },
        "ObjectChartPage": {
          "name": "ObjectChartPage",
          "controlAggregation": "endColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "freestylesapui5app.view.App",
      "type": "XML",
      "id": "fcl",
      "async": true
    }
  }
}
