{
    "xsappname": "freestylesapui5app",
    "tenant-mode": "dedicated",
    "description": "Security profile of called application",
    "scopes": [
      {
        "name": "uaa.user",
        "description": "UAA"
      },
      {
          "name": "$XSAPPNAME.accessDataService",
          "description": "Allows to access data service within freestylesapui5app app"
      },
      {
          "name": "$XSAPPNAME.accessApp",
          "description": "Allows to generally access freestylesapui5app app"
      }    
    ],
    "role-templates": [
      {
        "name": "Token_Exchange",
        "description": "UAA",
        "scope-references": [
          "uaa.user"
        ]
      },
      {
          "name": "user",
          "description": "Basic freestylesapui5app app user",
          "scope-references": ["$XSAPPNAME.accessApp", "$XSAPPNAME.accessDataService"]
      }
    ]
  }
  